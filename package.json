    "bluebird": "^3.0.5",
    "chai": "^3.2.0",
    "cssnext-loader": "^1.0.1",
    "eslint": "^3.17.1",
    "eslint-config-littlebits": "^0.6.2",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "prettier": "^1.7.3",
    "ramda": "^0.18.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-draggable": "^1.1.1",
    "react-hot-loader": "^1.2.4",
